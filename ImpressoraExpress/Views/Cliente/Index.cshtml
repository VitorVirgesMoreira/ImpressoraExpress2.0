@model List<ImpressoraExpressMVC.Models.ClienteViewModel>


@{
    ViewData["Title"] = "Cadastrar";
}


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Clientes Cadastrados</title>
</head>
<body>
    <h2>Clientes Cadastrados</h2>
    <div>
    <table class="table">
        <tr>
            <th>ID</th>
            <th>Nome</th>
            <th>CPF</th>
            <th>Email</th>
            <th>Data de Nascimento</th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>@item.ID</td>
                <td>@item.Nome</td>
                <td>@item.CPF</td>
                <td>@item.Email</td>
                <td>@item.DataNascimento.ToString("dd/MM/yyyy")</td>
            </tr>
        }

    </table>
    </div>
</body>
</html>

